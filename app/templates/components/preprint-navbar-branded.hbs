<div class="preprint-navbar navbar">
    <div class="container">
        <div class="navbar-header">
            {{#link-to "provider.index" class="navbar-brand" invokeAction=(action "click" "link" "Navbar - Brand")~}}
                <span class="navbar-image"></span>
                <span class="navbar-title">{{model.name}} {{t "global.preprints"}}</span>
            {{~/link-to}}
        </div>
        <div id="preprint-navbar-links" class="navbar-collapse collapse navbar-right">
            <ul class="nav navbar-nav">
                <li><a href="/myprojects/" class="" onbeforeclick={{action "click" "link" "Navbar - My OSF Projects"}}>{{t "components.preprint-navbar-branded.my_projects"}}</a></li>
                <li>{{#link-to "provider.submit" class="" invokeAction=(action "click" "link" "Navbar - Add preprint")}}{{t "global.add_preprint"}}{{/link-to}}</li>
                <li>{{#link-to "provider.discover" class="" invokeAction=(action "click" "link" "Navbar - Search")}}{{t "global.search"}}{{/link-to}}</li>
                {{navbar-auth-dropdown loginAction=(action 'login') headline=(t "components.preprint-navbar-branded.headline")}}
            </ul>
        </div>
    </div>
</div>
