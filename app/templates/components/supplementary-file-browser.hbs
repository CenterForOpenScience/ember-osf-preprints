{{#file-renderer download=selectedFile.links.download
    width="100%" height="700"}}
{{/file-renderer}}
<h5>{{selectedFile.name}}</h5>
<div class="row text-center">
    <div class="col-md-1">
        <button class="show-more-button text-muted" {{action 'moveLeft'}}>
            <i id = "show-more-icon" class="fa fa-caret-left"></i>
        </button>
    </div>
    <div class="col-md-10">
        {{#liquid-bind supplementList use="toLeft" as |list|}}
            {{#each list as |supplement|}}
                {{#if (eq supplement.kind "folder")}}
                    <i class="fa fa-folder fa-5x p-s" aria-hidden="true" data-toggle="tooltip" data-placement="bottom" title={{supplement.name}} href={{concat model.baseUrl model.project}}></i> {{!Actually make a link here}}
                {{else}}
                    <i class="fa fa-file-text fa-5x p-sm" aria-hidden="true" title={{supplement.name}} {{action 'changeFile' supplement}}></i>
                {{/if}}
            {{/each}}
        {{/liquid-bind}}
    </div>
    <div class="col-md-1">
        <button class="show-more-button text-muted" {{action 'moveLeft'}}>
            <i id = "show-more-icon" class="fa fa-caret-right"></i>
        </button>
    </div>
</div>