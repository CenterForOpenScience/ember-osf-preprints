{
  "name": "preprint-service",
  "version": "0.118.3",
  "description": "Center for Open Science Preprint Service",
  "license": "Apache-2.0",
  "author": "",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CenterForOpenScience/ember-osf-preprints.git"
  },
  "scripts": {
    "build": "ember build",
    "docs": "yuidoc",
    "start": "ember server",
    "check-style": "eslint .",
    "test": "yarn run check-style && ember test",
    "test:cover": "COVERAGE=true yarn test",
    "update-assets": "git submodule update --remote",
    "update-assets-hotfix": "yarn hotfix && yarn update-assets && git ci public/assets/osf-assets -m 'Updated assets'",
    "use-ember-osf-develop": "yarn upgrade @centerforopenscience/ember-osf@https://github.com/CenterForOpenScience/ember-osf.git#develop#$(date -u +%FT%TZ)",
    "use-ember-osf-next-interfaces": "yarn upgrade @centerforopenscience/ember-osf@https://github.com/CenterForOpenScience/ember-osf.git#release/next-interfaces#$(date -u +%FT%TZ)",
    "hotfix": "git flow hotfix start rename-me && npm --no-git-tag-version version patch && git ci package.json -m 'Bump version' && git br -m \"hotfix/$(npm version | head -1 | grep -o '[0-9]\\+\\.[0-9]\\+\\.[0-9]\\+')\""
  },
  "devDependencies": {
    "@centerforopenscience/ember-osf": "https://github.com/centerforopenscience/ember-osf#release/next-interfaces",
    "@centerforopenscience/eslint-config": "^2.0.0",
    "@centerforopenscience/osf-style": "1.7.0",
    "broccoli-asset-rev": "^2.4.5",
    "coveralls": "2.11.16",
    "ember-ajax": "^2.0.1",
    "ember-autosave": "^2.0.1",
    "ember-bootstrap": "^1.0.0-rc.1",
    "ember-browserify": "^1.2.0",
    "ember-cli": "2.18.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^6.7.1",
    "ember-cli-bootstrap-sassy": "0.5.8",
    "ember-cli-code-coverage": "^0.3.11",
    "ember-cli-dependency-checker": "^2.0.1",
    "ember-cli-eslint": "^4.2.0",
    "ember-cli-fake-server": "^0.3.2",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "1.4.1",
    "ember-cli-inline-content": "^0.4.1",
    "ember-cli-meta-tags": "4.0.0",
    "ember-cli-moment-shim": "^3.5.3",
    "ember-cli-postcss": "^3.5.2",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-template-lint": "^0.7.5",
    "ember-cli-uglify": "^1.2.0",
    "ember-collapsible-panel": "~1.0.0",
    "ember-cp-validations": "^3.5.1",
    "ember-data": "2.18.0",
    "ember-data-factory-guy": "^2.11.6",
    "ember-export-application-global": "^2.0.0",
    "ember-faker": "^1.1.1",
    "ember-get-config": "^0.2.4",
    "ember-i18n": "^5.0.2",
    "ember-link-action": "^0.0.36",
    "ember-load-initializers": "^1.0.0",
    "ember-metrics": "^0.6.3",
    "ember-moment": "^7.4.1",
    "ember-onbeforeunload": "^1.1.2",
    "ember-page-title": "^3.2.2",
    "ember-power-select": "^1.8.4",
    "ember-read-more": "^0.0.13",
    "ember-resolver": "^4.0.0",
    "ember-sanitize": "^2.0.2",
    "ember-sortable": "^1.8.2",
    "ember-source": "2.18.0",
    "ember-tag-input": "^1.2.1",
    "ember-sinon-qunit": "3.1.0",
    "ember-toastr": "^1.7.0",
    "ember-truth-helpers": "^1.2.0",
    "eslint": "^4.4.1",
    "eslint-plugin-ember": "^5.0.0",
    "liquid-fire": "^0.27.3",
    "loader.js": "^4.2.3",
    "postcss": "^5.1.0",
    "unicode-byte-truncate": "^1.0.0"
  },
  "engineStrict": true,
  "engines": {
     "node": ">=8"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/CenterForOpenScience/ember-osf-preprints/issues"
  },
  "homepage": "https://github.com/CenterForOpenScience/ember-osf-preprints#readme",
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "bootstrap-daterangepicker": "^2.1.23",
    "bootstrap-sass": "^3.3.7",
    "bower": "^1.8.2",
    "dropzone": "^5.3.0",
    "ember-cli-sass": "^7.0.0",
    "ember-collection": "^1.0.0-alpha.7",
    "ember-font-awesome": "^3.0.0",
    "font-awesome": "^4.7.0",
    "hint.css": "^2.5.0",
    "pagination-pager": "^3.0.0",
    "toastr": "^2.1.2"
  }
}
